        <hashTree>
          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.regex.Pattern
import java.util.regex.Matcher
def waitCountValue = vars.get(&quot;wait_count&quot;)
def wait_count_to_response = -1

if (waitCountValue != null &amp;&amp; !waitCountValue.isEmpty()) {
    def pattern = Pattern.compile(&quot;column0=(\\d+)&quot;)
    def matcher = pattern.matcher(waitCountValue)
    if (matcher.find()) {
        wait_count_to_response = matcher.group(1) as Integer
    } else {
        log.error(&quot;Не удалось найти число из таблицы t_mainapp в переменной wait_count: &quot; + waitCountValue)
    }
} else {
    log.error(&quot;Переменная wait_count не существует или пустая&quot;)
}

// Устанавливаем кастомное имя и данные
prev.setSampleLabel(&quot;wait_count_metric&quot;)
prev.setResponseData(wait_count_to_response.toString(), &quot;UTF-8&quot;)
prev.setBytes(wait_count_to_response.toString().bytes.length)

// Устанавливаем временные метки
long now = System.currentTimeMillis()
prev.setStartTime(now)
prev.setEndTime(now + wait_count_to_response)  // Теперь elapsed time = wait_count_value

// Статус и метрики
prev.setSuccessful(true)
prev.setResponseCodeOK()
prev.setResponseMessage(&quot;wait_count&quot;)
prev.setLatency(0)  // Время до первого байта
prev.setConnectTime(0)  // Время установки соединения</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PostProcessor>
