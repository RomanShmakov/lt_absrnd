        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="get_wait_journal" enabled="true">
          <stringProp name="dataSource">ydb_connection</stringProp>
          <stringProp name="query">SELECT count(c_status) FROM `journals/t_mainapp` where c_status = &quot;WAITING&quot;;</stringProp>
          <stringProp name="queryArguments"></stringProp>
          <stringProp name="queryArgumentsTypes"></stringProp>
          <stringProp name="queryTimeout"></stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="resultSetHandler">Store as String</stringProp>
